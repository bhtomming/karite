<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <title>{{ site.name }}|{{ site.keywords }}</title>
    <meta name="description" content="{{ site.description }}">
    <meta name="keywords" content="{{ site.keywords }}">
    <link href="/css/index.css" rel="stylesheet" type="text/css" />
    {% block headjsorcss %}
    {% endblock %}

</head>
<body>
<div id="header">
    <h1 style="display:none">
        <a href="" title="{{ site.name }}|{{ site.keywords }}">{{ site.name }}{{ site.keywords }}</a></h1>
    <div id="navigation">
        <div class="logo">
            {% if site.logo %}
                <p><img src="{{ site.logo }}" width="270" height="60"/></p>
            {% endif %}
        </div>
        <ul>
            <ul>
                <li {% if page=="" %} class="nav_in"{% endif %}><a href="/">首页</a></li>
                <li {% if page=="profile" %} class="nav_in"{% endif %}><a href="/profile/">可瑞特</a></li>
                <li {% if page=="brand" %} class="nav_in"{% endif %}><a href="/brand/">品牌中心</a></li>
                <li {% if page=="sucase" %} class="nav_in"{% endif %}><a href="/sucase/">加盟中心</a></li>
                <li {% if page=="contact" %} class="nav_in"{% endif %}><a href="/contact/">联系我们</a></li>
            </ul>
        </ul>
    </div>
</div>
{% block content %}
{% endblock %}
<div id="footer">
    <div id="copyright">
        <a href="http://{{ site.domain }}" title="{{ site.name }}">
            Copyright c {{ site.copyright }} | 客户专线：{{ site.tel }} | 备案号：{{ site.beian }}
        </a>
    </div>
</div>

{% block dzjs %}

{% endblock %}
</body>
</html>
