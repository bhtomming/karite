<!doctype html>
<html lang="zh-CN">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="renderer" content="webkit">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <title>{{ site.name }}</title>
    <meta name="description" content="{{ site.description }}">
    <meta name="keywords" content="{{ site.keywords }}">
    {% block stylesheet %}
        <link rel="stylesheet" href="/css/bootstrap.min.css">
        <link rel="stylesheet" href="/css/animate.min.css">
        <link rel="stylesheet" href="/css/app.css">
    {% endblock %}
</head>
<body>
{% block nav %}
<header id="index-header">
    <div class="navbar navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#dp-nav" aria-expanded="false">
                    <span class="sr-only">跳转菜单</span>
                    显示完整菜单
                </button>
                <a class="navbar-brand hidden-sm" href="#">NATURALBEANS</a>
            </div>
            <div class="collapse navbar-collapse" id="dp-nav">
                {#<div class="navbar-brand">NATURALBEANS</div>#}
                <ul class="nav navbar-nav center-block">
                    <li  class="nav-items"><a href="/" class="navbar-link"><img src="/images/nav_1.png" alt="" class="img-responsive"></a></li>
                    <li  class="nav-items"><a href="#dou-pu"  class="navbar-link"><img src="/images/nav_2.png" alt="" class="img-responsive"></a></li>
                    <li  class="nav-items"><a href="#brand"  class="navbar-link"><img src="/images/nav_3.png" alt="" class="img-responsive"></a></li>
                    <li  class="nav-items"><a href="#men-dian"  class="navbar-link"><img src="/images/nav_4.png" alt="" class="img-responsive"></a></li>
                    <li  class="nav-items"><a href="#join"  class="navbar-link"><img src="/images/nav_5.png" alt="" class="img-responsive"></a></li>
                    <li  class="nav-items visible-lg"><a href="#weibo"   tabindex="2"  data-toggle="modal"  data-target="#weibo-modal"><img src="/images/nav_6.png" alt="" class="img-responsive"/></a></li>
                    <li  class="nav-items visible-lg"><a href="#wechat"  tabindex="2"  data-toggle="modal" data-target="#wechat-modal"><img src="/images/nav_7.png" alt="" class="img-responsive"/></a></li>
                    <li  class="nav-items visible-lg"><a href="#"  ><img src="/images/nav_8.png" alt="" class="img-responsive"/></a></li>
                </ul>
            </div>
        </div>
    </div>
</header>
{% endblock %}
{% block content %}
{% endblock %}
<div id="footer">
    <div class="container-fluid">
        <div class="row">
            <div class="col-xs-8 col-sm-8">
                <ul class="copy-info">
                    <li>{{ site.description }}</li>
                    <li>{{ site.copyright }}: {{ site.domain }}</li>
                </ul>
                <div class="ew-img">
                    <div class="ewm pull-left">
                        <img src="/images/sm_wechat.png"  class="img-responsive center-block"/>
                    </div>
                    <div class="ewm pull-left">
                        <img src="/images/sm_weibo.png"  class="img-responsive center-block"/>
                    </div>
                </div>
            </div>
            <div class="col-xs-4 col-sm-4 bq-container">
                <div class="dj-bq"><img src="/images/dj_bq.png" alt="" class="img-responsive pull-right"></div>
            </div>
        </div>
    </div>
</div>
<div id="weibo-modal" class="modal fade" role="dialog" aria-labelledby="weibo-modal-label">
    <div class="modal-dialog" aria-hidden="true">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <div class="modal-title">扫描微博二维码到手机</div>
            </div>
            <div class="modal-body">
                <img src="/images/dj_weibo.jpg" alt="" class="img-responsive center-block">
            </div>
            <div class="modal-footer"></div>
        </div>
    </div>
</div>
<div id="wechat-modal" class="modal fade" role="dialog" aria-labelledby="wechat-modal-label">
    <div class="modal-dialog" aria-hidden="true">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <div class="modal-title">扫描微信二维码到手机</div>
            </div>
            <div class="modal-body">
                <img src="/images/dj_wechat.jpg" alt="" class="img-responsive center-block">
            </div>
            <div class="modal-footer"></div>
        </div>
    </div>
</div>
{% block javascript %}
    <script src="/js/jquery.js"></script>
    <script src="/js/bootstrap.js"></script>
    <script src="/js/wow.js"></script>
    <script>
		if (!(/msie [6|7|8|9]/i.test(navigator.userAgent))){
			new WOW().init();
		};
    </script>
{% endblock %}
</body>
</html>
