{% extends 'Layer.twig' %}
{% block nav %}
    <header id="about-header">
        <div class="year"><img src="/images/about/2015.png" alt="" class="img-responsive pull-right"></div>
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-5">
                    <div class="about-nav wow fadeIn" data-wow-duration="3" data-wow-delay="0.15s">
                        <a href="/" class="back-a"></a>
                    </div>
                </div>
            </div>
        </div>
    </header>
{% endblock %}
{% block content %}
   <div class="container-fluid about-container">
       <div class="row">
           <div class="col-md-5 wow fadeInLeft" data-wow-duration="5" data-wow-delay="0.25s">
               <img src="/images/about/dp_1.png" alt="" class="img-responsive center-block">
           </div>
           <div class="col-md-7 wow fadeInRightBig" data-wow-duration="5" data-wow-delay="0.5s">
               <img src="/images/about/dp_2.jpg" alt="" class="img-responsive">
           </div>
       </div>
       <div class="row">
           <div class="col-md-5 wow fadeInLeft" data-wow-duration="5" data-wow-delay="0.25s">
               <img src="/images/about/dp_3.png" alt="" class="img-responsive center-block">
           </div>
           <div class="col-md-7">
               <img src="/images/about/dp_4.jpg" alt="" class="img-responsive wow fadeInRight" data-wow-duration="5" data-wow-delay="0.5s">
               <img src="/images/about/dp_5.jpg" alt="" class="img-responsive wow flipInY" data-wow-duration="5" data-wow-delay="0.75s">
               <img src="/images/about/dp_6.jpg" alt="" class="img-responsive wow flipInX" data-wow-duration="5" data-wow-delay="1s">
               <div class="qs wow flipInY" data-wow-duration="5" data-wow-delay="1.25s"></div>
           </div>
       </div>
       <div class="back wow fadeIn" data-wow-duration="5" data-wow-delay="1.5s"><a href="/" class="back-a"></a></div>
   </div>
{% endblock %}
